{
  "name": "focustime-vue",
  "author": "Sander Vonk",
  "description": "Improve your productivity by scheduling out your time into more meaningful 'focus sessions' with FocusTime",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "(mkdir dist\\download 2>nul & move /Y dist_electron\\*.exe dist\\download\\ && ren dist\\download\\*.exe focustime.exe) & gh-pages -d dist",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "predeploy": "npm run build && npm run electron:build"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^3.8.3",
    "firebase": "^9.17.1",
    "jquery": "^3.6.3",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.47",
    "vue-router": "^4.0.3",
    "vue3-touch-events": "^4.1.0",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gh-pages": "^5.0.0",
    "mini-css-extract-plugin": "^2.5.3",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "homepage": "https://svonk.me/focusime-vue/"
}
